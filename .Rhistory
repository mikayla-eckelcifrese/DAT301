library(dplyr)
Weather = weatherAUS_xls
WeatherSample = (sample_n(Weather, 1000))
MaxTemp = WeatherSample[4]
MaxTempF = unlist(MaxTemp*(9/5)  + 32)#won't plot without doing unlist
Humidity3PM = unlist(WeatherSample[15])
cor(as.double(MaxTempF), as.double(Humidity3PM), use = "complete.obs")
library(ggplot2)
RainedToday = unlist(WeatherSample[23])
df = data.frame(MaxTempF, Humidity3PM, RainedToday)
ggplot(df, aes(x = MaxTempF, y = Humidity3PM, color = RainedToday)) + geom_point() + scale_color_manual(values = c("Yes" = "Blue", "No" = "Red")) + labs(title = "Max Daily Temp vs Afternoon Humidity")
